$rightoutbar {
	bindsym button4 nop
	bindsym button5 nop
	workspace_buttons no
	separator_symbol " "
	status_command i3blocks -c ~/dev/i3wmonarch/i3blocks/right_bottom_coincaps.conf
	position          bottom
	font pango:DejaVu Sans Mono for Powerline, Icons 14, FontAwesome 14
	id bar-11
	output $rightout
	tray_output none
	colors {
		focused_workspace #96a900 #859900 #fdf6e3
		inactive_workspace #369be2 #269be2 #ffffff
		active_workspace #96a900 #266E9E #fdf6e3
	}
}

