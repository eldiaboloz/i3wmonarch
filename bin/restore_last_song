#!/bin/bash
bp="$HOME/dev/i3wmonarch/temp"
if [ ! -e "${bp}/last_song_cmus.path" ]; then
	exit 1
fi
if [ ! -e "${bp}/last_song_cmus.time" ]; then
	exit 1
fi

cmusdata=`cmus-remote -Q`
if [[ $? != 0 ]]; then
	start_audioplayer
	sleep 2
fi

cmus-remote --file "$(cat ${bp}/last_song_cmus.path)"
cmus-remote --seek "$(cat ${bp}/last_song_cmus.time)"
